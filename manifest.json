{
  "name": "InstaSee",
  "version": "2.0.1",
  "manifest_version": 2,
  "description": "See Instagram media in bigger size",
  "homepage_url": "http://onepagecraft.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://instagram.com/*",
        "https://www.instagram.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://instagram.com/*",
    "http://www.instagram.com/*",
    "https://instagram.com/*",
    "https://www.instagram.com/*"
  ]
}